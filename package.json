{
  "name": "twentytwenty-child",
  "version": "1.0.0",
  "main": "index.js",
  "keywords": [],
  "author": "tonborzenko",
  "license": "ISC",
  "description": "This is TwentyTwenty child Theme for Wordpress and Woocommerce",
  "scripts": {
    "sass:build": "node-sass -r --output-style expanded src/scss -o assets/css",
    "sass:watch": "npm run sass:build && npm run sass:build -- -w",
    "rollup": "rollup -c -w",
    "start": "npm-run-all --parallel sass:watch rollup"
  },
  "devDependencies": {
    "autosize": "^4.0.2",
    "clean-css-cli": "^4.3.0",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "rollup": "^1.27.8",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-node-resolve": "^5.2.0"
  }
}
